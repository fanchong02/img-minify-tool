{
  "name": "img-minify-tool",
  "version": "1.0.0",
  "author": "fanchong",
  "license": "ISC",
  "description": "一行命令即可实现所有图片无损压缩， 大大提高工作效率",
  "main": "index.js",
  "keywords": [
    "img",
    "minify",
    "svg",
    "png",
    "jpeg",
    "jpg",
    "gif",
    "无损压缩",
    "image"
  ],
  "scripts": {
    "minify": "sh minify.sh",
    "minify:node": "node index.js --bundlePath"
  },
  "bin": {
    "minify": "node .index.js --bundlePath"
  },
  "dependencies": {
    "imagemin": "^7.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^8.0.0",
    "minimist": "^1.2.6"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=7.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fanchong02/img-minify-tool.git"
  }
}
